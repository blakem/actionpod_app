<% opacity = '65' %>
<% percent = '0.65' %>
<img src="/images/icons/users_two_add_48.png" style="opacity:<%= percent %>;filter:alpha(opacity=<%= opacity %>)"
onmouseover="this.style.opacity=1;this.filters.alpha.opacity=100"
onmouseout="this.style.opacity=<%= percent %>;this.filters.alpha.opacity=<%= opacity %>">
<img src="/images/icons/users_two_48.png" style="opacity:<%= percent %>;filter:alpha(opacity=<%= opacity %>)"
onmouseover="this.style.opacity=1;this.filters.alpha.opacity=100"
onmouseout="this.style.opacity=<%= percent %>;this.filters.alpha.opacity=<%= opacity %>">
<img src="/images/icons/user_two_delete_48.png" style="opacity:<%= percent %>;filter:alpha(opacity=<%= opacity %>)"
onmouseover="this.style.opacity=1;this.filters.alpha.opacity=100"
onmouseout="this.style.opacity=<%= percent %>;this.filters.alpha.opacity=<%= opacity %>">
